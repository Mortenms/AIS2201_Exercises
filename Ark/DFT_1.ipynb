   "source": [
    "import numpy as np\n",
    "from numpy import cos, pi, sin, exp\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "N = 10\n",
    "n = np.arange(N)\n",
    "x = cos(2*pi/5*n)\n",
    "\n",
    "m_array = np.arange(-5, 6, 1)\n",
    "Xm = np.zeros(len(m_array))\n",
    "\n",
    "for m in m_array:\n",
    "    Xm[m] = np.sum(x*np.e**(-2j*pi*m*n/N))\n",
    "    \n",
    "plt.stem(m_array, np.abs(Xm))\n"
   ]
  }
